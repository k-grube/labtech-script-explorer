{
  "name": "labtech-script-explorer",
  "version": "0.4.0",
  "description": "React App that decodes and displays LabTech Scripts",
  "keywords": [
    "LabTech",
    "ConnectWise",
    "Automate",
    "React",
    "ConnectWise Automate"
  ],
  "engines": {
    "node": "6.11.4"
  },
  "repository": {
    "url": "https://github.com/k-grube/labtech-script-explorer.git",
    "type": "git"
  },
  "homepage": "https://k-grube.github.io/labtech-script-explorer",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-typecheck": "^3.6.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "6.26.0",
    "better-npm-run": "^0.1.0",
    "body-parser": "^1.18.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "compression": "^1.7.2",
    "concurrently": "^3.5.1",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.16.2",
    "express-validator": "^4.3.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "labtech-script-decode": "^0.7.1",
    "lodash": "^4.17.15",
    "morgan": "^1.10.0",
    "object-assign": "4.1.1",
    "piping": "^1.0.0-rc.4",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "pretty-error": "^2.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.6.1",
    "pushstate-server": "^3.1.0",
    "react": "^15.6.1",
    "react-dev-utils": "^4.2.3",
    "react-dom": "^15.6.1",
    "react-dropzone": "^4.2.8",
    "react-fa": "^5.0.0",
    "react-json-tree": "^0.11.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-semantic-redux-form": "^1.2.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.2.3",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.3.0",
    "semantic-ui-react": "^0.74.2",
    "style-loader": "0.18.2",
    "superagent": "^3.8.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3",
    "winston": "^2.4.0",
    "winston-papertrail": "^1.0.5"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0"
  },
  "betterScripts": {
    "start-web-dev": {
      "command": "node scripts/start.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start-api-dev": {
      "command": "node scripts/start-api.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3030
      }
    },
    "start-api-prod": {
      "command": "node scripts/start-api.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": 3000
      }
    }
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm run start-web-dev\" \"npm run start-api-dev\"",
    "start-api-dev": "better-npm-run start-api-dev",
    "start-web-dev": "better-npm-run start-web-dev",
    "postinstall": "npm run build",
    "prod": "better-npm-run start-api-prod",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
